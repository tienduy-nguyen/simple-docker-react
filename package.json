{
  "name": "back-end-exercise",
  "version": "1.0.0",
  "description": "An exercise for back-end candidates",
  "main": "src/index.js",
  "author": "Tien Duy",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register ./src/main.ts",
    "setup:db": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register ./src/database/setup-test-database.ts",
    "test": "jest",
    "test:integration": "yarn setup:db && jest --config ./test/integration/jest-integration.json",
    "test:e2e": "yarn setup:db && jest --config ./test/e2e/jest-e2e.json",
    "seed": "ts-node -r tsconfig-paths/register ./src/seed.ts",
    "migration": "ts-node -r tsconfig-paths/register ./src/database/migration.ts"
  },
  "dependencies": {
    "argon2": "0.27.1",
    "connect-redis": "5.1.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-async-handler": "1.1.4",
    "express-session": "1.17.1",
    "ioredis": "4.23.0",
    "jsonwebtoken": "8.5.1",
    "sqlite": "4.0.19",
    "sqlite3": "5.0.2",
    "tsyringe": "4.6.0"
  },
  "devDependencies": {
    "@types/connect-redis": "0.0.16",
    "@types/express": "4.17.11",
    "@types/express-session": "1.17.3",
    "@types/faker": "5.1.7",
    "@types/ioredis": "4.22.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "14.14.32",
    "@types/sqlite3": "3.1.7",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jest": "24.1.8",
    "eslint-plugin-prettier": "3.3.1",
    "faker": "5.4.0",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "reflect-metadata": "0.1.13",
    "supertest": "6.1.3",
    "ts-jest": "26.5.2",
    "ts-node-dev": "1.1.6",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.3"
  }
}
